<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <stroke android:width="0.5dp" android:color="@color/lib_color_font8" />
            <solid android:color="@color/lib_color_font8" />

            <corners android:radius="0dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="0.5dp" android:color="@color/lib_color_font8_trans60" />
            <solid android:color="@color/lib_color_font8_trans60" />
            <corners android:radius="0dp" />
        </shape>
    </item>
</selector>
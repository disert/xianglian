<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <stroke android:width="0.5dip" android:color="@color/lib_color_bg4" />
            <solid android:color="@color/lib_transparent" />
            <corners android:radius="2dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="0.5dip" android:color="@color/lib_color_bg4_trans40" />
            <solid android:color="@color/lib_transparent" />
            <corners android:radius="2dp" />
        </shape>
    </item>
</selector>